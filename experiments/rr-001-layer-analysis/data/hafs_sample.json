    "sura_name_en": "Al-Fatihah",
    "sura_name_ar": "الفاتحة",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 1,
    "aya_text": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "aya_text_emlaey": "بسم الله الرحمن الرحيم"
  },
  {
    "id": "1:2",
    "jozz": 1,
    "page": 1,
    "sura_no": 1,
    "sura_name_en": "Al-Fatihah",
    "sura_name_ar": "الفاتحة",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 2,
    "aya_text": "الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ",
    "aya_text_emlaey": "الحمد لله رب العالمين"
  },
  {
    "id": "1:3",
    "jozz": 1,
    "page": 1,
    "sura_no": 1,
    "sura_name_en": "Al-Fatihah",
    "sura_name_ar": "الفاتحة",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 3,
    "aya_text": "الرَّحْمَٰنِ الرَّحِيمِ",
    "aya_text_emlaey": "الرحمن الرحيم"
  },
  {
    "id": "1:4",
    "jozz": 1,
    "page": 1,
    "sura_no": 1,
    "sura_name_en": "Al-Fatihah",
    "sura_name_ar": "الفاتحة",
    "line_start": 4,
    "line_end": 4,
    "aya_no": 4,
    "aya_text": "مَالِكِ يَوْمِ الدِّينِ",
    "aya_text_emlaey": "مالك يوم الدين"
  },
  {
    "id": "1:5",
    "jozz": 1,
    "page": 1,
    "sura_no": 1,
    "sura_name_en": "Al-Fatihah",
    "sura_name_ar": "الفاتحة",
    "line_start": 5,
    "line_end": 5,
    "aya_no": 5,
    "aya_text": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
    "aya_text_emlaey": "إياك نعبد وإياك نستعين"
  },
  {
    "id": "1:6",
    "jozz": 1,
    "page": 1,
    "sura_no": 1,
    "sura_name_en": "Al-Fatihah",
    "sura_name_ar": "الفاتحة",
    "line_start": 6,
    "line_end": 6,
    "aya_no": 6,
    "aya_text": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ",
    "aya_text_emlaey": "اهدنا الصراط المستقيم"
  },
  {
    "id": "1:7",
    "jozz": 1,
    "page": 1,
    "sura_no": 1,
    "sura_name_en": "Al-Fatihah",
    "sura_name_ar": "الفاتحة",
    "line_start": 7,
    "line_end": 7,
    "aya_no": 7,
    "aya_text": "صِرَاطَ الَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ الْمَغْضُوبِ عَلَيْهِمْ وَلَا الضَّالِّينَ",
    "aya_text_emlaey": "صراط الذين أنعمت عليهم غير المغضوب عليهم ولا الضالين"
  },
  {
    "id": "2:1",
    "jozz": 1,
    "page": 2,
    "sura_no": 2,
    "sura_name_en": "Al-Baqarah",
    "sura_name_ar": "البقرة",
    "line_start": 1,
    "line_end": 1,
    "aya_no": 1,
    "aya_text": "الم",
    "aya_text_emlaey": "الم"
  },
  {
    "id": "2:2",
    "jozz": 1,
    "page": 2,
    "sura_no": 2,
    "sura_name_en": "Al-Baqarah",
    "sura_name_ar": "البقرة",
    "line_start": 2,
    "line_end": 2,
    "aya_no": 2,
    "aya_text": "ذَٰلِكَ الْكِتَابُ لَا رَيْبَ فِيهِ هُدًى لِّلْمُتَّقِينَ",
    "aya_text_emlaey": "ذلك الكتاب لا ريب فيه هدى للمتقين"
  },
  {
    "id": "2:3",
    "jozz": 1,
    "page": 2,
    "sura_no": 2,
    "sura_name_en": "Al-Baqarah",
    "sura_name_ar": "البقرة",
    "line_start": 3,
    "line_end": 3,
    "aya_no": 3,
    "aya_text": "الَّذِينَ يُؤْمِنُونَ بِالْغَيْبِ وَيُقِيمُونَ الصَّلَاةَ وَمِمَّا رَزَقْنَاهُمْ يُنفِقُونَ",
    "aya_text_emlaey": "الذين يؤمنون بالغيب ويقيمون الصلاة ومما رزقناهم ينفقون"
  }
]
